{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-app.html",
    "src/my-kc.html",
    "src/my-survey-grid.html",
    "src/my-survey-view.html",
    "src/shared-styles.html",
    "src/submission-dialog.html",
    "src/login-dialog.html",
    "src/pw-change-dialog.html",
    "src/my-profile.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
